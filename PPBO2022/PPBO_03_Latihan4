public class PPB0_03_Latihan4
{
    public static void main(String[] args)
    {
        int arr[] = {5, 1, 4, 2, 3};
        int i = 0;
        do
        {
            System.out.println("elemen ke-" + (i + 1) + " = " + arr[i]);
            i++;
        } while (i < 5);
    }
}
